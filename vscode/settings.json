{
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "file-icons",
  "files.autoSave": "onFocusChange",
  "editor.minimap.enabled": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "workbench.sideBar.location": "right",
  "editor.fontFamily": "Fira Code, CascadiaCode Nerd Font, Cascadia Code, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.validate.enable": true,
  "javascript.validate.enable": true,
  "zenMode.hideTabs": false,
  "editor.suggestSelection": "first",
  "editor.occurrencesHighlight": false,
  "editor.lineNumbers": "relative",
  /*
   * Color settings
   */
  "workbench.colorTheme": "Gruvbox Material Dark",
  "terminal.integrated.fontWeightBold": "500",
  "workbench.colorCustomizations": {
    "editor.background": "#1f1f1f",
    "statusBar.background": "#1f1f1f",
    "editorGroupHeader.tabsBackground": "#1f1f1f",
    "tab.inactiveBackground": "#1f1f1f",
    "titleBar.activeBackground": "#1f1f1f"
  },
  /*
   * VsCodeVim settings
   */
  "vim.surround": true,
  "vim.sneak": true,
  "vim.leader": "space",
  "vim.visualstar": true,
  "vim.camelCaseMotion.enable": true,
  "vim.replaceWithRegister": true,
  "vim.useCtrlKeys": true,
  "vim.highlightedyank.enable": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["k", "j"],
      "commands": [
        "extension.vim_escape",
        "editor.action.formatDocument",
        "workbench.action.files.save"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "e"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "E"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "after": ["[", "{", "[", "{", "[", "{", "[", "{", "[", "{"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "after": [
        "g",
        "g",
        "/",
        "e",
        "x",
        "p",
        "o",
        "r",
        "t",
        " ",
        "<enter>",
        "w",
        "w",
        "<leader>",
        "s",
        "r"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "s", "r"],
      "commands": [
        {
          "command": "editor.action.goToReferences"
        }
      ]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["K"],
      "after": ["i", "<enter>", "<esc>"]
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z", "z", "o"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z", "z", "o"]
    },
    {
      "before": ["<leader>", "d", "a"],
      "after": ["v", "i", "<", "o", "W", "h", "d", "<esc>", "<leader>", "h"]
    },
    {
      "before": ["<leader>", "i"],
      "after": ["i", " ", "<esc>", "i"]
    },
    {
      "before": ["<leader>", "a", "c"],
      "after": ["A", ",", "<esc>", "j", "^"]
    },
    {
      "before": ["<leader>", "a", "p"],
      "after": ["A", ",", "<enter>"]
    },
    {
      "before": ["<leader>", "d", "p"],
      "after": ["V", "$", "%", "d"]
    },
    {
      "before": ["<leader>", "a", "s"],
      "after": ["a", " ", "<esc>"]
    },
    {
      "before": ["<leader>", "%"],
      "commands": [
        {
          "command": "editor.emmet.action.matchTag"
        }
      ]
    },
    {
      "before": ["<leader>", "`"],
      "after": ["f", "`", "c", "i", "`"]
    },
    {
      "before": ["<leader>", "\""],
      "after": ["f", "\"", "c", "i", "\""]
    },
    {
      "before": ["<leader>", "'"],
      "after": ["f", "'", "c", "i", "'"]
    },
    {
      "before": ["<leader>", ")"],
      "after": ["f", ")", "c", "i", ")"]
    },
    {
      "before": ["<leader>", "("],
      "after": ["f", "(", "c", "i", "("]
    },
    {
      "before": ["<leader>", "{"],
      "after": ["f", "{", "c", "i", "{"]
    },
    {
      "before": ["<leader>", "}"],
      "after": ["f", "}", "c", "i", "}"]
    },
    {
      "before": ["<leader>", "["],
      "after": ["f", "[", "c", "i", "["]
    },
    {
      "before": ["<leader>", "]"],
      "after": ["f", "]", "c", "i", "]"]
    },
    {
      "before": ["<leader>", "p", "l"],
      "after": ["o", "<esc>", "p"]
    },
    {
      "before": ["<leader>", "p", "e"],
      "after": ["E", "a", " ", "<esc>", "v", "p"]
    },
    {
      "before": ["<leader>", "p", "p"],
      "after": ["A", ",", "<esc>", "p"]
    },
    {
      "before": ["<leader>", "f"],
      "after": ["/"]
    },
    {
      "before": ["<leader>", "F"],
      "after": ["?"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "t", "o"],
      "commands": [":tabonly"]
    },
    {
      "before": ["<C-u>"],
      "after": ["H", "z", "z"]
    },
    {
      "before": ["<C-d>"],
      "after": ["L", "z", "z"]
    }
  ],
  "cSpell.userWords": [
    "aboutme",
    "collapsable",
    "COMMS",
    "containerregistry",
    "datacapture",
    "ECHAL",
    "EEFGR",
    "Eitel",
    "Errored",
    "favourite",
    "firestore",
    "gameplay",
    "gameplays",
    "gcloud",
    "googleapis",
    "gserviceaccount",
    "initialising",
    "Komo",
    "leaderboard",
    "Microservices",
    "nowrap",
    "Parens",
    "Pathing",
    "rebass",
    "receiptuploader",
    "sanitised",
    "scratchandwins",
    "Scratchie",
    "scroller",
    "secretcodes",
    "SENDGRID",
    "Sidedrawer",
    "Sidesheet",
    "Spotify",
    "Subsea",
    "Treeview",
    "Unticked",
    "unvalidated",
    "wordsorless",
    "Xunit"
  ],
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "html.validate.scripts": false,
  "scm.alwaysShowRepositories": true,
  "window.title": "",
  "files.exclude": {
    "bin": true,
    "obj": true,
    "out": true
  },
  "git.autofetch": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 8,
  "workbench.statusBar.visible": true,
  "editor.renderControlCharacters": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[csharp]": { "editor.defaultFormatter": "ms-dotnettools.csharp" },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "settingsSync.ignoredExtensions": ["dbaeumer.vscode-eslint"],
  "redhat.telemetry.enabled": false,
  "workbench.editor.wrapTabs": true,
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "debug.javascript.terminalOptions": {
    "skipFiles": ["<node_internals>/**", "**.js"]
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.enabled": false,
  "debug.javascript.suggestPrettyPrinting": false,
  "editor.renderWhitespace": "none",
  "workbench.activityBar.visible": false,
  "explorer.confirmDelete": false,
  "editor.bracketPairColorization.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "git.allowForcePush": true,
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "extensions.ignoreRecommendations": true,
  "window.zoomLevel": 1
}
