{
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "file-icons",
  "files.autoSave": "onFocusChange",
  "editor.minimap.enabled": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "workbench.sideBar.location": "right",
  "editor.fontFamily": "Fira Code, CascadiaCode Nerd Font, Cascadia Code, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.validate.enable": true,
  "javascript.validate.enable": true,
  "zenMode.hideTabs": false,
  "editor.suggestSelection": "first",
  "editor.occurrencesHighlight": false,
  "editor.lineNumbers": "relative",
  /*
   * Color settings
   */
  "workbench.colorTheme": "Gruvbox Material Dark",
  "terminal.integrated.fontWeightBold": "500",
  "workbench.colorCustomizations": {
    "editor.background": "#1f1f1f",
    "statusBar.background": "#1f1f1f",
    "editorGroupHeader.tabsBackground": "#1f1f1f",
    "tab.inactiveBackground": "#1f1f1f",
    "titleBar.activeBackground": "#1f1f1f",
    "sideBar.foreground": "#D4BE98"
  },
  "cSpell.userWords": [
    "aboutme",
    "behaviour",
    "collapsable",
    "COMMS",
    "containerregistry",
    "datacapture",
    "draftail",
    "ECHAL",
    "EEFGR",
    "Eitel",
    "Errored",
    "favourite",
    "firestore",
    "flexi",
    "Flexis",
    "fullscreen",
    "gameplay",
    "gameplays",
    "gcloud",
    "Giphy",
    "googleapis",
    "gserviceaccount",
    "gsutil",
    "Guids",
    "hubspot",
    "initialising",
    "inoremap",
    "Insta",
    "jank",
    "Komo",
    "Konva",
    "leaderboard",
    "Microservices",
    "nnoremap",
    "noremap",
    "nowrap",
    "nvim",
    "Parens",
    "Pathing",
    "prefs",
    "rebass",
    "receiptuploader",
    "sanitised",
    "scratchandwins",
    "Scratchie",
    "scroller",
    "secretcodes",
    "SENDGRID",
    "Sidedrawer",
    "Sidesheet",
    "Signup",
    "Spotify",
    "Subsea",
    "Treeview",
    "Unpublish",
    "Unpublishing",
    "Unticked",
    "unvalidated",
    "userflow",
    "vsync",
    "wordsorless",
    "Xunit"
  ],
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "html.validate.scripts": false,
  "scm.alwaysShowRepositories": true,
  "window.title": "",
  "files.exclude": {
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".idea": true,
    ".next": true,
    "android": true,
    "bin": true,
    "build": true,
    "ios": true,
    "linux": true,
    "macos": true,
    "mobile_ui.iml": true,
    "node_modules": true,
    "obj": true,
    "out": true,
    "web": true,
    "windows": true
  },
  "git.autofetch": true,
  "editor.cursorBlinking": "solid",
  "workbench.statusBar.visible": true,
  "editor.renderControlCharacters": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[csharp]": { "editor.defaultFormatter": "ms-dotnettools.csharp" },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "settingsSync.ignoredExtensions": ["dbaeumer.vscode-eslint"],
  "redhat.telemetry.enabled": false,
  "workbench.editor.wrapTabs": true,
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "debug.javascript.terminalOptions": {
    "skipFiles": ["<node_internals>/**", "**.js"]
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.enabled": false,
  "editor.renderWhitespace": "none",
  "workbench.activityBar.visible": false,
  "explorer.confirmDelete": false,
  "editor.bracketPairColorization.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "git.allowForcePush": true,
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "extensions.ignoreRecommendations": true,
  "gitlens.currentLine.enabled": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "dart.flutterHotReloadOnSave": "always",
  "dart.hotReloadOnSave": "always",
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "dart.openDevTools": "flutter",
  "gitlens.fileAnnotations.command": "blame",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, yarn.lock",
    "pubspec.yaml": ".packages, .metadata, pubspec.lock, analysis_options.yaml"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "git.autoRepositoryDetection": false,
  "git.mergeEditor": true,
  "window.zoomLevel": 1,
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/bin",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  }
}
